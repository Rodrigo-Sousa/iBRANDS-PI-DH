                            <a href="#" class="remove">
                                <span aria-hidden="true">&times;</span>
                            </a>
                        </td>
                    </tr>
                    <tr class="table-total">
                        <th class="total-th-cart-shopping">Total à pagar:</th>
                        <td class="total-td-cart-shopping" colspan="5">R$ 5.033,60</td>
                    </tr>
                    <tr class="buying">
                        <td colspan="6" class="finished-buying">
                           <a href="#">Finalizar Compra</a>
                        </td>
                    </tr>
                </tbody>
            </table>
            <!-- Fechamento div container -->
        </div>

        <%- include("./partials/footer") %>