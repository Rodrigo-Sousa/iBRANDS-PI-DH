                </thead>
                <tbody>
                    <tr>
                        <th scope="row">1</th>
                        <td>B450 AORUS M</td>
                        <td>
                            <select name="quantidade" id="primeiro">
                                <option value="1">1</option>
                                <option value="2" selected>2</option>
                                <option value="3">3</option>
                                <option value="4">4</option>
                            </select>
                        </td>
                        <!-- <td>2</td> -->
                        <td>R$ 649,00</td>
                        <td>R$ 1.298,00</td>
                        <td class="title-remove">
                            <a href="#" class="remove">
                                <span aria-hidden="true">&times;</span>
                            </a>
                        </td>
                    </tr>
                    <tr>
                        <th scope="row">2</th>
                        <td>Upgrade1277</td>
                        <td>
                            <select name="quantidade" id="primeiro">
                                <option value="1" selected>1</option>
                                <option value="2">2</option>
                                <option value="3">3</option>
                                <option value="4">4</option>
                            </select>
                        </td>
                        <!-- <td>1</td> -->
                        <td>R$ 1.590,90</td>
                        <td>R$ 1.590,90</td>
                        <td class="title-remove">
                            <a href="#" class="remove">
                                <span aria-hidden="true">&times;</span>
                            </a>
                        </td>
                    </tr>
                    <tr>
                        <th scope="row">3</th>
                        <td>B450M Steel</td>
                        <td>
                            <select name="quantidade" id="primeiro">
                                <option value="1">1</option>
                                <option value="2">2</option>
                                <option value="3" selected>3</option>
                                <option value="4">4</option>
                            </select>
                        </td>
